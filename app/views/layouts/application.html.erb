<!DOCTYPE html>
<html>
<head>
  <title><%= render_title %></title>  
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <meta name="viewport" content="width=630;">
</head>
<body>
	<%= render :partial => "layouts/nav" %>
	<div id="content">
		<!-- Required by Devise -->
		<p class="notice"><%= notice %></p>
		<p class="alert"><%= alert %></p>
		<%= yield %>
	</div>
</body>
</html>
